{
  "buildCommand": "git lfs install && git lfs pull",
  "cleanUrls": true,
  "builds": [
    { "src": "Build/**", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "outputDirectory": "Build"
  "headers": [
    {
      "source": "/Build/(.*)\\.unityweb",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    },
    {
      "source": "/Build/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
